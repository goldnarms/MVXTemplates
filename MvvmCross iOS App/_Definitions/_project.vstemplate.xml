<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>MvvmCross iOS App (iOS only)</Name>
    <Description>MvvmCross Version: 5.0.1
    
A project template for a MvvmCross iOS application. This template is dependent on the 'MvvmCross Core' project template (or similar).</Description>
    <DefaultName>iOS</DefaultName>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>iOS</ProjectSubType>
    <SortOrder>90</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>icon.png</Icon>
    <TemplateID>$guid1$</TemplateID>
    <NumberOfParentCategoriesToRollUp>9</NumberOfParentCategoriesToRollUp>
    <PromptForSaveOnCreation>true</PromptForSaveOnCreation>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="$projectname$.csproj" File="MvvmCross.iOS.App.csproj" ReplaceParameters="true" />
  </TemplateContent>
  <WizardExtension>
    <Assembly>NuGet.VisualStudio.Interop, Version=1.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
    <FullClassName>NuGet.VisualStudio.TemplateWizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <packages repository="extension" repositoryId="VSIX.Luke Pothier.3b4a0b56-7f92-4f5e-8de3-fe5bb43c7678">
      <package id="Cirrious.FluentLayout" version="2.5.0" />
      <package id="MvvmCross" version="5.0.1" />
      <package id="MvvmCross.Binding" version="5.0.1" />
      <package id="MvvmCross.CodeAnalysis" version="5.0.1" />
      <package id="MvvmCross.Core" version="5.0.1" />
      <package id="MvvmCross.Platform" version="5.0.1" />
      <package id="MvvmCross.Plugin.Json" version="5.0.1" />
      <package id="Newtonsoft.Json" version="10.0.2" />
    </packages>
  </WizardData>
</VSTemplate>
<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>MvvmCross iOS App</Name>
    <Description>A project template for a MvvmCross iOS application. This template is dependent on the 'MvvmCross Core' project template being in the same solution.</Description>
    <DefaultName>iOS</DefaultName>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>iOS</ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>template-icon.png</Icon>
    <TemplateID>$guid1$</TemplateID>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="$projectname$.csproj" File="MvvmCross.iOS.App.csproj" ReplaceParameters="true">
    </Project>
    <CustomParameters>
      <CustomParameter Name="$TemplateName$" Value="MvvmCross iOS App" />
      <CustomParameter Name="$TemplateID$" Value="$guid2$" />
      <CustomParameter Name="$TemplateType$" Value="Project" />
    </CustomParameters>
  </TemplateContent>
</VSTemplate>
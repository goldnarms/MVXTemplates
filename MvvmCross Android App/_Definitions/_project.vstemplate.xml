<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>MvvmCross Android App (Android only)</Name>
    <Description>MvvmCross Version: 4.4.0
    
A project template for a MvvmCross Android application. This template is dependent on the 'MvvmCross Core' project template (or similar).</Description>
    <DefaultName>Android</DefaultName>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>Android</ProjectSubType>
    <SortOrder>90</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>icon.png</Icon>
    <TemplateID>$guid1$</TemplateID>
    <NumberOfParentCategoriesToRollUp>9</NumberOfParentCategoriesToRollUp>
    <PromptForSaveOnCreation>true</PromptForSaveOnCreation>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="$projectname$.csproj" File="MvvmCross.Android.App.csproj" ReplaceParameters="true" />
  </TemplateContent>
